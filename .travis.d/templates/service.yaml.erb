apiVersion: v1
kind: Service
metadata:
  labels:
    name: <%= app['uid'] %>
    heritage: Biodomes
  name: <%= app['suid'] or app['uid'] %>
spec:
  type: NodePort
  ports:
    -
      name: http
      port: <%= app['port'] or 80 %>
      protocol: TCP
      targetPort: <%= app['target_port'] or 3000 %>
  selector:
    name: <%= app['uid'] %>
