apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: app-ingress
spec:
  rules:
  <% biodomes.each do |_, biodome| %>
  <% biodome['apps'].each do |app_name, app| %>
  - host: <%= app['host'] %>
    http:
      paths:
      - backend:
          serviceName: <%= app['uid'] %>
          servicePort: <%= app['port'] or 80 %>
  <% end %>
  <% end %>
